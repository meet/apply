<!DOCTYPE html>
<html>
<head>
  <title><%= content_for?(:title) ? yield(:title) : 'Apply to MEET' %></title>
  <%= stylesheet_link_tag 'application', :media => 'all' %>
  <%= javascript_include_tag :defaults %>
  <%= yield :head %>
  <%= csrf_meta_tag %>
</head>
<body>
<% if @current_user %><div id="logged-in"><%= @current_user %> | <%= link_to 'logout', logout_path %></div><% end %>

<%= yield %>

</body>
</html>
