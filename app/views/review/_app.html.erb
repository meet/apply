<div id="application">
  
<h1><%= @call.identify(@app) %></h1>

<% @call.app_class.form_columns.each do |c| %>
  <% next if @call.identity_columns_a.include? c.name %>
  <div class="app-block-<%= c.type %>">
  <h3><%= @call.app_class.human_attribute_name(c.name) %></h3>
  <p><%= app_response(@app, c) || '&mdash;'.html_safe %></p>
  </div>
<% end %>

</div>
